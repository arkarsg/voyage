{
  "name": "voyage-app",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start --dev-client",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "format": "prettier --no-error-on-unmatched-pattern --write \"**/*.{tsx,ts,js}\"",
    "lint": "esw \"**/*.{tsx,ts,js}\" --color",
    "lint:watch": "yarn lint --watch",
    "test": "jest --passWithNoTests --coverage",
    "precommit": "yarn format && yarn lint && yarn test",
    "reset": "rm -r -f node_modules && rm -r -f ios && yarn && npx --yes expo prebuild --platform ios --clean"
  },
  "dependencies": {
    "@react-native-firebase/app": "^19.1.1",
    "@react-native-firebase/auth": "^19.1.1",
    "@react-native-google-signin/google-signin": "^10.1.2",
    "@realm/react": "^0.6.0",
    "@tamagui-extras/core": "^1.12.2",
    "@tamagui-extras/form": "^1.17.2",
    "@tamagui/config": "^1.94.1",
    "@testing-library/react-native": "^12.4.5",
    "dotenv": "^16.4.5",
    "expo": "^49.0.8",
    "expo-build-properties": "~0.8.3",
    "expo-constants": "~14.4.2",
    "expo-dev-client": "~2.4.8",
    "expo-firebase": "^0.0.0-alpha.0",
    "expo-linear-gradient": "~12.3.0",
    "expo-linking": "~5.0.2",
    "expo-router": "^2.0.0",
    "expo-secure-store": "~12.3.1",
    "expo-splash-screen": "~0.20.5",
    "expo-status-bar": "~1.6.0",
    "firebase": "^10.9.0",
    "jest-expo": "~49.0.0",
    "prop-types": "^15.8.1",
    "react": "18.2.0",
    "react-hook-form": "^7.51.2",
    "react-native": "0.72.10",
    "react-native-gesture-handler": "~2.12.0",
    "react-native-get-random-values": "~1.9.0",
    "react-native-safe-area-context": "4.6.3",
    "react-native-screens": "~3.22.0",
    "react-native-svg": "13.9.0",
    "react-native-web": "~0.19.6",
    "react-test-renderer": "^18.2.0",
    "realm": "12.0.0",
    "tamagui": "^1.94.1"
  },
  "devDependencies": {
    "@babel/core": "^7.22.5",
    "@babel/plugin-proposal-decorators": "^7.22.5",
    "@realm/babel-plugin": "^0.1.1",
    "@types/jest": "^29.5.12",
    "@types/react": "~18.2.13",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard-with-typescript": "^43.0.1",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-n": "^15.0.0 || ^16.0.0 ",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-native": "^4.1.0",
    "eslint-watch": "^8.0.0",
    "jest": "^29.2.1",
    "prettier": "^3.2.5",
    "typescript": "*"
  },
  "jest": {
    "preset": "jest-expo",
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|@react-native(-community)?)|expo(nent)?|@expo(nent)?/.*|@expo-google-fonts/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|react-native-svg)"
    ]
  },
  "license": "Apache-2.0",
  "private": true
}
